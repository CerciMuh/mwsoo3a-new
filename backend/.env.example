# Backend environment variables
# Copy to backend/.env and edit as needed

# Server
PORT=4000

# Local Universities JSON (optional absolute or relative path)
# Default resolution looks for world_universities.json at repo root
UNIVERSITIES_JSON_PATH=

# Cognito (backend token validation)
COGNITO_REGION=eu-central-1
COGNITO_USER_POOL_ID=eu-central-1_XXXXXXX
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Cognito (for backend token validation)
COGNITO_REGION=us-east-1
COGNITO_USER_POOL_ID=us-east-1_XXXXXXX
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx


